<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextView">
        <attr name="fiw_text" format="string" />
        <attr name="fiw_textColor" format="color" />
        <attr name="fiw_textSize" format="dimension" />
        <attr name="fiw_inputType" >
            <enum name="fiw_number" value="1" />
            <enum name="fiw_text" value="2" />
            <enum name="fiw_password" value="3" />
        </attr>
    </declare-styleable>
    
    <declare-styleable name="QQStepView">
        <attr name="outerColor" format="color" />
        <attr name="innerColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="stepTextSize" format="dimension" />
        <attr name="stepTextColor" format="color"/>
    </declare-styleable>

</resources>
